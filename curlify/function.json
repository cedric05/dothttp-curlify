{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "routePrefix": "customPrefix",
      "direction": "in",
      "name": "req",
      "route": "{*route}",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/curlify/index.js"
}
